{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "jta:block/british_rail_platform"
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "jta:block/british_rail_platform",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "jta:block/british_rail_platform",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "jta:block/british_rail_platform",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}